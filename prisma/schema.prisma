generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AngBao {
  id           String   @id @default(uuid())
  name         String
  age          Int
  relationship String
  lifeStatus   String
  energyScore  Int
  roastMessage String
  blessing      String
  secretMessage String   @default("")
  createdAt     DateTime @default(now())
}
